<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.hackathonapp">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Activity.state_wise_home"></activity>
        <activity android:name=".Activity.history_home" />
        <activity android:name=".Activity.tracker_home" />
        <activity android:name=".Activity.tracker_splash" />
        <activity android:name=".Activity.news_home" />
        <activity android:name=".Activity.news_splash" />
        <activity android:name=".Activity.handwash_home" />
        <activity android:name=".Activity.handwash_splash" />
        <activity android:name=".Activity.grocery_splash" />
        <activity android:name=".Activity.grocery_stores" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>